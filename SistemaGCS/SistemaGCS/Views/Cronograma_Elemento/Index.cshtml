@model List<SistemaGCS.Models.Cronograma_Elemento>
@{
    ViewBag.Title = "Index";
}

<h2>Cronograma Elemento</h2>
<h2 class="pagegeader">
    <a href="~/Cronograma_Elemento/Agregar" class="btn btn-info text-right">Nuevo Registro</a>

</h2>
<br />
<br />
@using (Html.BeginForm("Index", "Cronograma_Elemento", FormMethod.Post))
{

}

<table id="datatable" class="display" style="width:100%">
    <thead>
        <tr>
            <th>Id</th>
            <th>Proyecto </th>
            <th>Elemento C </th>
            <th>Inicio</th>
            <th>Termino</th>
            <th>Acciones</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var m in Model)
        {
            <tr>
                <td>@(m.Id_cronoele)</td>
                <td>@(m.Proyecto.Nombre)</td>
                <td>@(m.Elemento_Configuracion.Nombre)</td>
                <td>@(m.Fechainicio)</td>
                <td>@(m.Fechafin)</td>
                                <td>

                    <a href="~/Cronograma_Elemento/Agregar/@m.Id_cronoele" class="btn btn-success">Editar</a>

                </td>
            </tr>
        }
    </tbody>
    <p></p>
</table>

